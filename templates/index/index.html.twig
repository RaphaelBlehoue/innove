{% extends 'base.html.twig' %}
{% block title %}
    {{ 'Intégrateur de solutions innovantes' }}
{% endblock %}
{% block base_nav %} {{ include('front/includes/nav.html.twig',{'nav_class':'c-layout-header-4 c-layout-header-default-mobile'}) }} {% endblock %}
{% block slide %}
    {{ include('front/includes/slide.html.twig') }}
{% endblock %}
{% block body %}
    <!-- BEGIN: CONTENT/CABINET -->
    <div class="c-content-box c-size-md c-no-padding c-bg-white">
        <div class="c-content-feature-4">
            <div class="c-bg-parallax c-feature-bg c-content-right c-arrow c-border-left-white" style="background-image: url({{ asset('build/web/static/home_about.jpg') }})"></div>
            <div class="c-content-area c-content-left"></div>
            <div class="container">
                <div class="c-feature-content c-left">
                    <div class="c-content-v-center">
                        <div class="c-wrapper">
                            <div class="c-body">
                                <div class="c-content-title-1 animated wow fadeInDown" data-wow-delay="0.2s">
                                    <h3 class="c-font-uppercase c-font-bold c-left">{{ 'La société INNOVE-IT & CONSULTING'|upper }}</h3>
                                    <div class="c-line-left"></div>
                                </div>
                                <div class="c-content animated wow fadeInDown" data-wow-delay="0.7s">
                                    <p class="c-margin-b-30 c-left">
                                        {{ 'Est une équipe innovante avec des talents, des compétences, de l\'expérience des savoir-faire et des savoirs-être engagés à réussir vos projets à court et à long termes dans le domaine des TIC. Elle est spécialisée dans les environnements systèmes: système d’information, systèmes de gestion informatisée, réseau & télécoms.' }}
                                    </p>
                                    <a href="{{ path('about') }}" class="btn btn-lg c-btn-green c-btn-border-2x c-btn-square c-btn-bold wow animated fadeIn" data-wow-delay="1s">{{ 'découvrir qui nous sommes en détail'|upper }}</a>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <!-- END: CONTENT/CABINET -->

    <!-- BEGIN: CONTENT/SOLUTION QUE NOUS OFFRONS -->
    <div class="c-content-box c-size-md c-bg-img-center c-bg-grey-1">
        <div class="container">
            <div class="c-content-title-1 wow animated fadeIn">
                <h3 class="c-center c-font-uppercase c-font-bold">{{ 'les solutions que nous offrons par domaine de gestion'|upper }}</h3>
                <div class="c-line-center c-theme-bg"></div>
                <div class="row">
                    <div class="c-content-blog-post-card-1-grid">
                        <div class="row">
                            {% for i in 1..8 %}
                                <div class="col-md-4">
                                    <div class="c-content-blog-post-card-1 c-option-2 c-bordered box-shadow c-shadow">
                                        <div class="c-media c-content-overlay">
                                            <div class="c-overlay-wrapper">
                                                <div class="c-overlay-content">
                                                    <a href="#"><i class="icon-link"></i></a>
                                                </div>
                                            </div>
                                            <img class="c-overlay-object img-responsive"  src="http://via.placeholder.com/500x350" alt="">
                                        </div>
                                        <div class="c-body">
                                            <div class="c-title c-font-bold c-font-uppercase">
                                                <a href="#">{{'Titre de la solutions ici'}}</a>
                                            </div>
                                            <p>{{ 'petite descriptions de la solutions ici'|striptags('<a>')|truncate(110, true, '...')|raw }}</p>
                                            <div class="c-content-divider">
                                                <i class="icon-dot"></i>
                                            </div>
                                            <div class="c-center">
                                                <a href="" class="btn btn-sm c-btn-green-1 c-btn-uppercase c-btn-bold c-btn-border-1x c-btn-square">{{  'je suis intéressé' }}</a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            {% endfor %}
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
